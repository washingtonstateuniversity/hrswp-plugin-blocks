{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "hrswp/sidebar",
  "title": "Sidebar",
  "category": "layout",
  "description": "Display content in a sidebar-style layout (two-thirds and one-third).",
  "keywords": [
    "sidebar",
    "columns"
  ],
  "textdomain": "default",
  "attributes": {
    "verticalAlignment": {
      "type": "string"
    },
    "isStackedOnMobile": {
      "type": "boolean",
      "default": true
    },
    "sidebarPosition": {
      "type": "string",
      "default": "right"
    }
  },
  "supports": {
    "anchor": true,
    "align": true,
    "html": false
  },
  "editorScript": "hrswp-blocks-script",
  "editorStyle": "hrswp-blocks-editor-style",
  "style": "hrswp-blocks-style"
}